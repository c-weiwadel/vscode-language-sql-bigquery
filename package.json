{
  "name": "sql-bigquery",
  "displayName": "SQL (BigQuery)",
  "description": "BigQuery SQL language support in Visual Studio Code",
  "version": "1.9.0",
  "publisher": "shinichi-takii",
  "author": {
    "name": "Shinichi Takii"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.31.0"
  },
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "keywords": [
    "language",
    "sql",
    "bigquery",
    "grammar",
    "syntax",
    "snippets"
  ],
  "homepage": "https://github.com/shinichi-takii/vscode-language-sql-bigquery/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/shinichi-takii/vscode-language-sql-bigquery.git"
  },
  "bugs": {
    "url": "https://github.com/shinichi-takii/vscode-language-sql-bigquery/issues"
  },
  "icon": "images/BigQuery.png",
  "contributes": {
    "languages": [
      {
        "id": "sql-bigquery",
        "aliases": [
          "SQL (BigQuery)",
          "sql-bigquery"
        ],
        "extensions": [
          ".sql",
          ".ddl",
          ".dml",
          ".bqsql",
          ".bqddl",
          ".bqdml"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "light": "./images/bigquery-logo-blue.png",
          "dark": "./images/bigquery-logo-blue.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "sql-bigquery",
        "scopeName": "source.sql-bigquery",
        "path": "./syntaxes/sql-bigquery.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "sql-bigquery",
        "path": "./snippets/sql-bigquery.snippets.json"
      }
    ]
  }
}
